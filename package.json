{
  "name": "nextwind",
  "description": "TypeScript skeleton for Next.js, TailwindCSS and Zustand",
  "repository": "https://github.com/en3sis/nextwind",
  "version": "1.0.0",
  "author": "en3sis",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev-turbo": "NODE_OPTIONS='--inspect' next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier --write '**/*.tsx' '**/*.ts'",
    "prepare": "husky install"
